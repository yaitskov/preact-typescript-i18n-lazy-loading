  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": [
    "src"
  ]
}
